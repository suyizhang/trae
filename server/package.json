{
  "name": "weixin-server",
  "version": "1.0.0",
  "description": "微信小程序后端服务",
  "main": "dist/bootstrap.js",
  "scripts": {
    "start": "node ./bootstrap.ts",
    "dev": "cross-env NODE_ENV=local midway-bin dev --ts",
    "build": "midway-bin build -c",
    "test": "midway-bin test --ts",
    "lint": "eslint --ext .ts src/ test/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "keywords": [
    "midway",
    "koa",
    "typescript",
    "weixin"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@midwayjs/bootstrap": "^3.0.0",
    "@midwayjs/core": "^3.0.0",
    "@midwayjs/decorator": "^3.0.0",
    "@midwayjs/info": "^3.0.0",
    "@midwayjs/koa": "^3.0.0",
    "@midwayjs/logger": "^2.14.0",
    "@midwayjs/validate": "^3.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@midwayjs/cli": "^2.0.0",
    "@midwayjs/mock": "^3.0.0",
    "@types/jest": "^29.2.0",
    "@types/koa": "^2.13.4",
    "@types/node": "^16.11.22",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.2.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.0"
  }
}